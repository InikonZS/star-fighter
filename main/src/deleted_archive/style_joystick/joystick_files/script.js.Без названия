      }

      if (this.reqFilter) {
        this.childList = this.childList.filter(function (it) {
          return it.isExists;
        });
        this.reqFilter = false;
      }

      this.childList.forEach(function (it) {
        it.render(gl, props);
      });
    }
  }, {
    key: "process",
    value: function process(deltaTime, props) {
