          dir = this.pos.subVector(playerPosition).normalize();
          this.atack = true;
        } else {
          dir = this.pos.subVector(playerPosition).mul(-1);
          dir.x = dir.x + Math.random() * 10 - 5;
          dir.y = dir.y + Math.random() * 10 - 5;
