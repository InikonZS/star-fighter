  function App(parentNode) {
    var _this = this;

    _classCallCheck(this, App);

    this.parentNode = parentNode;
    this.stWidth = 640;
    this.stHeight = 480;

    if (document.documentElement.clientWidth < 768) {
      this.stWidth = 320;
      this.stHeight = 240;
